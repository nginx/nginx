#!/bin/sh

. auto/options
. auto/init
. auto/sources

test -d $OBJS || mkdir $OBJS
echo > $NGX_AUTO_CONFIG_H

if [ $DEBUG = YES ]; then
    have=NGX_DEBUG . auto/have
fi

have=NGX_USE_HTTP_FILE_CACHE_UNIQ . auto/have
have=NGX_SUPPRESS_WARN . auto/have


if [ "$PLATFORM" != win32 ]; then
    . auto/headers
fi

. auto/os/conf
. auto/modules

. auto/cc
. auto/lib/conf

if [ "$PLATFORM" != win32 ]; then
    . auto/threads
fi

. auto/make
. auto/lib/make

if [ "$PLATFORM" != win32 ]; then
    . auto/unix
fi

have=NGX_SMP . auto/have

. auto/summary
